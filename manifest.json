{
	"manifest_version": 2,
	"name": "zIndexhelper",
	"version": "1.0",
	"description": "An Zindex Helper",
	"background": {
    "scripts": ["background.js"],
    "persistent": false
  },
	"browser_action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs", "http://*/*", 
		"https://*/*"
	]
}